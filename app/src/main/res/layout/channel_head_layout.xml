<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.combinehead.MainActivity" xmlns:app="http://schemas.android.com/apk/res/com.inspur.emmcloud">


    
    <RelativeLayout 
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@android:color/black"
        >
        <com.example.combinehead.RoundAngleImageView 
            android:id="@+id/lefttop"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_alignParentTop="true"
            android:src="@drawable/ic_launcher"
            app:roundWidth="5dp"
            app:roundHeight="5dp"
            app:roundrighttop="false"
            app:roundleftdown="false"
            app:roundrightdown="false"/>
         <com.example.combinehead.RoundAngleImageView 
            android:id="@+id/righttop"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_toRightOf="@id/lefttop"
            android:src="@drawable/ic_launcher"
            app:roundWidth="5dp"
            app:roundHeight="5dp"
            app:roundlefttop="false"
            app:roundleftdown="false"
            app:roundrightdown="false"/>
          <com.example.combinehead.RoundAngleImageView 
            android:id="@+id/leftdown"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_below="@id/lefttop"
            android:src="@drawable/ic_launcher"
            app:roundWidth="5dp"
            app:roundHeight="5dp"
            app:roundlefttop="false"
            app:roundrighttop="false"
            app:roundrightdown="false"/>
           <com.example.combinehead.RoundAngleImageView 
            android:id="@+id/rightdown"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_toRightOf="@id/leftdown"
            android:layout_alignTop="@id/leftdown"
            android:src="@drawable/ic_launcher"
            app:roundWidth="5dp"
            app:roundHeight="5dp"
            app:roundlefttop="false"
            app:roundrighttop="false"
            app:roundleftdown="false"
            />
    </RelativeLayout>

</RelativeLayout>
